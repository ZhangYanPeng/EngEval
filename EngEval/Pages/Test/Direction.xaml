<Window x:Class="EngEval.Pages.Test.Direction"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EngEval.Pages.Test"
        mc:Ignorable="d"
        ResizeMode="NoResize" 
        WindowStyle="None"
        WindowStartupLocation="CenterOwner"
        Title="Direction" Height="900" Width="1400">
    <DockPanel Margin="80">
        <TextBlock Name="partType" FontSize="40" DockPanel.Dock="Top"></TextBlock>
        <DockPanel Height="30" DockPanel.Dock="Top"></DockPanel>
        <MediaElement Name="directAudio"></MediaElement>
        <TextBlock Name="directText" FontSize="30" TextWrapping="Wrap"></TextBlock>
    </DockPanel>
</Window>
